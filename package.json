{
  "name": "stories",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -w bin -w server bin/www",
    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production node bin/www",
    "lint": "eslint --fix \"**/*.js\" --ignore-path=.gitignore",
    "knex": "knex",
    "knex:rollback": "knex -- migrate:rollback --all",
    "knex:migrate": "knex -- migrate:latest",
    "knex:reset": "npm-run-all knex:rollback knex:migrate",
    "knex:seed": "knex -- seed:run",
    "knex:prepare": "npm-run-all knex:migrate knex:seed",
    "test": "cross-env DOTENV_EXT=testing npm-run-all knex:migrate test:run",
    "test:run": "npm-run-all -pr dev wdio",
    "wdio": "npm-run-all knex:seed wdio:run",
    "wdio:run": "wdio run wdio.conf.js"
  },
  "dependencies": {
    "@apollo/client": "^3.2.0",
    "@apollo/link-schema": "^2.0.0-beta.3",
    "@apollo/react-hooks": "^4.0.0",
    "@apollo/react-ssr": "^4.0.0",
    "@babel/register": "^7.11.5",
    "@types/react": "^16.9.49",
    "apollo-server-express": "^2.17.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "browserslist": "^4.14.3",
    "commander": "^6.1.0",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "knex": "^0.21.5",
    "next": "^9.5.3",
    "nodemailer": "^6.4.11",
    "nprogress": "^0.2.0",
    "objection": "^2.2.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redis": "^3.0.2",
    "sass": "^1.26.11",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@wdio/cli": "^6.5.2",
    "@wdio/local-runner": "^6.5.2",
    "@wdio/mocha-framework": "^6.5.0",
    "@wdio/spec-reporter": "^6.4.7",
    "@wdio/sync": "^6.5.0",
    "babel-eslint": "^10.1.0",
    "chromedriver": "^85.0.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "wdio-chromedriver-service": "^6.0.4"
  }
}
